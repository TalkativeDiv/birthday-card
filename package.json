{
  "name": "c86p",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5300",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "npx eslint src",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "lint-full": "concurrently \"npm run lint\" \"npm run prettier\"",
    "test": "concurrently \"npm run lint:fix\" \"npm run prettier:fix\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@playwright/test": "^1.24.2",
    "@types/alpinejs": "^3.7.0",
    "@types/fabric": "^4.5.11",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "autoprefixer": "^10.4.8",
    "eslint": "^8.21.0",
    "husky": "^8.0.0",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.7",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  },
  "dependencies": {
    "alpinejs": "^3.10.3",
    "concurrently": "^7.3.0",
    "fabric": "^5.2.1",
    "fabricjs": "^1.0.0"
  }
}
